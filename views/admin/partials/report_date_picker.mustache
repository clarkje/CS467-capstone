<div>
  <form class="form-inline" role="form" method="GET" action="{{formAction}}">
  <div class="form-group">
    <label>Start Date</label>
    <div class="input-group date" data-provide="datepicker" data-date-autoclose="true">
      <input type="text" class="form-control" autoclose="true" name="startDate" value="{{startDate}}">
      <div class="input-group-addon">
        <span class="glyphicon glyphicon-th"></span>
      </div>
    </div>
  </div>

  <div class="form-group">
    <label>End Date</label>
    <div class="input-group date" data-provide="datepicker" data-date-autoclose="true">
      <input type="text" class="form-control" autoclose="true" name="endDate" value="{{endDate}}">
      <div class="input-group-addon">
        <span class="glyphicon glyphicon-th"></span>
      </div>
    </div>
  </div>

  <button type="submit" class="btn btn-default">Update Report</button>
  </form>
</div>
